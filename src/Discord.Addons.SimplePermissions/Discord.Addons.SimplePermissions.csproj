<Project Sdk="Microsoft.NET.Sdk" ToolsVersion="15.0">
    <Import Project="..\..\DiscordAddonsCore.props" />
    <PropertyGroup>
        <AssemblyName>Discord.Addons.SimplePermissions</AssemblyName>
        <VersionPrefix>1.0.0</VersionPrefix>
        <VersionSuffix>rc2</VersionSuffix>
        <Description>Discord.Net module for command permission management.</Description>
        <Authors>Joe4evr</Authors>
        <PackageTags>discord;commands;permissions</PackageTags>
        <TargetFrameworks>$(TargetFrameworks);netstandard1.6</TargetFrameworks>
    </PropertyGroup>
    <ItemGroup>
        <None Include="README.md" />
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="Microsoft.Extensions.DependencyModel" Version="2.0.0" />
        <PackageReference Include="System.Diagnostics.Process" Version="4.3.0" />
    </ItemGroup>
</Project>