<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup Label="Configuration">
        <AllowUnsafeBlocks>False</AllowUnsafeBlocks>
        <TreatWarningsAsErrors>False</TreatWarningsAsErrors>
        <WarningLevel>0</WarningLevel>
    </PropertyGroup>
    <PropertyGroup>
        <Description>Simple usage of Audio for Discord.Net 1.0.</Description>
        <VersionPrefix>0.1.3</VersionPrefix>
        <VersionSuffix>rc</VersionSuffix>
        <Authors>Joe4evr</Authors>
        <TargetFramework>netstandard1.5</TargetFramework>
        <AssemblyName>Discord.Addons.SimpleAudio</AssemblyName>
        <PackageId>Discord.Addons.SimpleAudio</PackageId>
        <RootNamespace>Discord.Addons.SimpleAudio</RootNamespace>
        <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
        <PackageProjectUrl>https://github.com/Joe4evr/Discord.Addons</PackageProjectUrl>
        <PackageLicenseUrl>http://opensource.org/licenses/MIT</PackageLicenseUrl>
        <RepositoryType>git</RepositoryType>
        <RepositoryUrl>git://github.com/Joe4evr/Discord.Addons</RepositoryUrl>
        <PackageTags>discord;commands;audio</PackageTags>
        <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Discord.Addons.Preconditions" Version="0.2.5-rc" />
        <PackageReference Include="Discord.Net.Commands" Version="1.0.0-rc3-00766" />
        <PackageReference Include="Discord.Net.WebSocket" Version="1.0.0-rc3-00766" />
        <PackageReference Include="System.Diagnostics.Process" Version="4.3.0" />
        <PackageReference Include="System.ValueTuple" Version="4.3.0" />
    </ItemGroup>
    <PropertyGroup>
        <GenerateDocumentationFile>True</GenerateDocumentationFile>
    </PropertyGroup>
    <!--<Import Project="..\Discord.Addons.Core\Discord.Addons.Core.projitems" Label="Shared" />-->
</Project>