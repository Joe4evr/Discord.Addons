{
  "version": "0.7.13",
  "description": "Base classes for text-based games in Discord",
  "authors": [ "Joe4evr" ],

  "packOptions": {
    "owners": [ "Joe4evr" ],
    "repository": {
      "type": "git",
      "url": "https://github.com/Joe4evr/Discord.Addons"
    },
    "licenseUrl": "http://opensource.org/licenses/MIT",
    "files": {
      "exclude": [ "Example/**" ]
    },
    "tags": [ "discord", "game" ]
  },

  "configurations": {
    "Release": {
      "buildOptions": {
        "compile": {
          "exclude": [ "Example/**" ]
        },
        "optimize": true,
        "xmlDoc": true
      }
    }
  },

  "dependencies": {
    "Discord.Net.Core": "1.0.0-beta2-*",
    "Discord.Net.Commands": "1.0.0-beta2-*"
  },

  "frameworks": {
    "netstandard1.3": {
      "dependencies": {
        "NETStandard.Library": {
          "version": "1.6.0"
        }
      }
    }
  }
}
